
## open bash specific files
alias 'open_bash'='vim ~/.bashrc'
alias 'open_commands'='vim ~/.bash_commands'
alias 'open_aliases'='vim ~/.bash_aliases'
alias 'open_rtw_file'='vim ~/.ros_team_ws_rc'

## sourcing
alias 'source_bash'='source ~/.bashrc'
#ros
alias 'source_foxy'='source /opt/ros/foxy/setup.bash'
alias 'source_galactic'='source /opt/ros/galactic/setup.bash'
alias 'source_humble'='source /opt/ros/humble/setup.bash'
alias 'source_rolling'='source /opt/ros/rolling/setup.bash'
